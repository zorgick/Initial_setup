{
  "name": "initial-setup",
  "version": "0.3.0",
  "description": "My custom setup of a react project",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production node scripts/build",
    "build:analyze": "cross-env NODE_ENV=production DEBUG_BUNDLE=true webpack -p --progress",
    "start": "cross-env NODE_ENV=development node scripts/start",
    "typejs": "tsc -p ./tsconfig.json",
    "test": "jest --watch",
    "test:build": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "release-patch": "npm version patch -m 'chore(*) patch version'",
    "release-minor": "npm version minor -m 'chore(*) minor version'",
    "release-major": "npm version major -m 'chore(*) major version'",
    "docs": "jsdoc2md",
    "checkall": "npm run typejs && lint-staged && npm run test:build"
  },
  "keywords": [],
  "author": "Zakhar Kononov (zorgizorgik@gmail.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zorgick/Initial_setup.git"
  },
  "dependencies": {
    "@loadable/component": "^5.11.0",
    "@loadable/server": "^5.11.0",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "core-js": "^3.5.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-manifest-helpers": "^0.6.0",
    "history": "^4.10.1",
    "i18next": "^19.0.2",
    "i18next-fetch-backend": "^2.2.0",
    "isomorphic-fetch": "^2.2.1",
    "module-alias": "^2.2.2",
    "prod-console": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "^5.2.1",
    "react-helmet-async": "^1.0.4",
    "react-i18next": "^11.2.6",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "regenerator": "^0.14.2",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-async-to-generator": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@loadable/babel-plugin": "^5.11.0",
    "@loadable/webpack-plugin": "^5.7.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.23",
    "@types/redux-logger": "^3.0.7",
    "@types/webpack-env": "^1.14.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "browser-env": "^3.3.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.1",
    "css-loader": "^3.3.2",
    "cssnano": "^4.1.10",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-style-loader": "^5.1.0",
    "jest": "^24.9.0",
    "jsdoc-to-markdown": "^5.0.3",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.19.1",
    "puppeteer": "^2.0.0",
    "react-dev-utils": "^10.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^3.0.0",
    "standard": "^14.3.1",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "svgo-loader": "^2.2.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.3",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "_moduleAliases": {
    "scripts": "./scripts",
    "config": "./config"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jestSetup.js"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.js"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "src/**/*.test.js"
    ],
    "globals": [
      "__BROWSER__",
      "__SERVER__"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "jsxBracketSameLine": true
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ],
    "*.css": [
      "stylelint \"src/**/*.css\" --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run checkall"
    }
  }
}
